<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="100dp"
        android:height="50dp"
        android:bottom="0dp"
        android:top="0dp"
        android:start="5dp">
        <shape>
            <solid android:color="#fff" />
            <corners android:radius="2dp"/>
        </shape>
    </item>
    <item
        android:width="20dp"
        android:height="20dp"
        android:left="0dp"
        android:top="-10dp">

        <shape android:shape="oval">
            <solid android:color="#FFFFFF"/>

        </shape>
    </item>
</layer-list>
